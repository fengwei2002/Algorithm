#include <algorithm>
#include <iostream>
#include <vector>

using namespace std;
/*2021-07-20-00-12*/

class Solution {
   public:
    int singleNumber(vector<int>& nums) {
        int ans = 0;
        for (auto item : nums) ans ^= item;
        return ans;
    }
};

// 交换律：a ^ b ^ c <=> a ^ c ^ b

// 任何数于0异或为任何数 0 ^ n => n

// 相同的数异或为0: n ^ n => 0